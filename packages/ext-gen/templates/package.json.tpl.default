{
  "name": "{packageName}",
  "product": "ext",
  "version": "{version}",
  "description": "{description}",
  "scripts": {
    "start": "npm run desktop",
    "desktop": "webpack-dev-server --profile=desktop --environment=development -w --progress --open",
    "phone": "webpack-dev-server --profile=phone --environment=development -w --progress --open",
    "build-desktop-testing": "ext-build app build desktop testing",
    "build-desktop-production": "ext-build app build desktop production",
    "build-phone-testing": "ext-build app build phone testing",
    "build-phone-production": "ext-build app build phone production",
    "createview": "ext-build generate viewpackage $npm_package_extbuild_buildprofile $VIEW"
  },
  "extbuild": {
    "defaultprofile": "desktop",
    "defaultenvironment": "development"
  },
  "dependencies": {<tpl if="modern == true">
    "{npmScope}/ext-modern": "^6.6.0",
    "{npmScope}/ext-modern-{modernTheme}": "^6.6.0",</tpl><tpl if="classic == true">    "{npmScope}/ext-classic": "^6.6.0",
    "{npmScope}/ext-classic-{classicTheme}": "^6.6.0",
</tpl>    "{npmScope}/ext": "^6.6.0"
  },
  "devDependencies": {
    "{npmScope}/ext-webpack-plugin": "~0.0.1",
    "command-line-args": "^5.0.2",
    "portfinder": "^1.0.13",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "repository": {
    "type": "git",
    "url": "{gitRepositoryURL}"
  },
  "keywords": [{keywords}],
  "author": "{authorName}",
  "license": "{license}",
  "bugs": {
    "url": "{bugsURL}"
  },
  "homepage": "{homepageURL}"
}
